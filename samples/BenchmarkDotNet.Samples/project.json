{
  "title": "BenchmarkDotNet.Samples",

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "keyFile": "../../src/strongNameKey.snk",
        "strongName": true
      }
    }
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "CLASSIC" ],
        "allowUnsafe": true
      },
      "frameworkAssemblies": {
        "System.Reflection": "4.0.0.0"
      },
      "dependencies": {
        "BenchmarkDotNet.Diagnostics.Windows": {
          "target": "project"
        }
      }
    },
    "netcoreapp1.1": {
      "buildOptions": {
        "define": [ "CORE" ],
        "allowUnsafe": true
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        },
        "System.ComponentModel.EventBasedAsync": "4.0.11"
      }
    },
    "uap10.0": {
      "buildOptions": {
        "define": [ "CORE", "UAP" ],
        "allowUnsafe": true
      },
      "dependencies": {
        "Microsoft.NETCore.UniversalWindowsPlatform": "5.2.2",
        "System.Threading.Tasks": "4.3.0"
      }
    }
  },
  "dependencies": {
    "BenchmarkDotNet": {
      "target": "project"
    }
  }
}